          },
          "typeVersion": 1
        },
        {
          "id": "3",
          "name": "Item Lists",
          "type": "n8n-nodes-base.itemLists",
          "position": [
            650,
            300
          ],
          "parameters": {
            "options": {},
            "batchSize": 1,
            "operation": "splitInBatches"
          },
          "typeVersion": 1,
